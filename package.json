{
  "name": "vue-range-multi",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@8.9.0",
  "description": "A range vue component that support one or more thumb",
  "author": "wiidede <wiixdede@gmail.com>",
  "license": "MIT",
  "funding": "https://wiidede.space/sponsor",
  "homepage": "https://github.com/wiidede/vue-range-multi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiidede/vue-range-multi.git"
  },
  "bugs": "https://github.com/wiidede/vue-range-multi/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite playground",
    "preview": "vite preview playground",
    "build:play": "vite build playground",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "pnpm lint && pnpm typecheck && pnpm build && bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.24",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.8.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.9.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
