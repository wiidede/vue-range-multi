{
  "name": "vue-range-multi",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.15.3",
  "description": "A range vue component that support one or more thumb",
  "author": "wiidede <wiixdede@gmail.com>",
  "license": "MIT",
  "funding": "https://wiidede.space/sponsor",
  "homepage": "https://github.com/wiidede/vue-range-multi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiidede/vue-range-multi.git"
  },
  "bugs": "https://github.com/wiidede/vue-range-multi/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite playground",
    "dev:pkg": "vite playground --mode pkg",
    "preview": "vite preview playground",
    "build:play": "vite build playground",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "pnpm lint && pnpm typecheck && pnpm build && bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@iconify-json/carbon": "^1.1.30",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.19",
    "@unocss/eslint-config": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.4",
    "bumpp": "^9.3.0",
    "defu": "^6.1.4",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.3",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-inspect": "^0.8.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
